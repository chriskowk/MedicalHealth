<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="integration.config" type="JobController.Configuration.IntegrationSection, JobController"/>
  </configSections>
  <integration.config>
    <schedulers>
      <jobtype name="TaskJob" fullname="MyJob.TaskJob" basepath="E:\VSTS\MedicalHealth" jobkey="TaskJob" jobgroup="TaskJobGroup" schedulerfile="JobConfig.xml" customer="中山眼科"/>
      <jobtype name="TaskJobA" fullname="MyJob.TaskJobA" basepath="E:\MedicalHealthSY" jobkey="TaskJobA" jobgroup="TaskJobAGroup" schedulerfile="JobAConfig.xml" customer="省医"/>
      <jobtype name="TaskJobB" fullname="MyJob.TaskJobB" basepath="E:\MedicalHealthBasicRC" jobkey="TaskJobB" jobgroup="TaskJobBGroup" schedulerfile="JobBConfig.xml" customer="市十二"/>
      <jobtype name="TaskJobC" fullname="MyJob.TaskJobC" basepath="E:\MedicalHealthGH" jobkey="TaskJobC" jobgroup="TaskJobCGroup" schedulerfile="JobCConfig.xml" customer="光华口腔"/>
      <jobtype name="TaskJobD" fullname="MyJob.TaskJobD" basepath="E:\MedicalHealthS1" jobkey="TaskJobD" jobgroup="TaskJobDGroup" schedulerfile="JobDConfig.xml" customer="市一"/>
      <jobtype name="TaskJobE" fullname="MyJob.TaskJobE" basepath="E:\MedicalHealthSGS1" jobkey="TaskJobE" jobgroup="TaskJobEGroup" schedulerfile="JobEConfig.xml" customer="韶关市一"/>
    </schedulers>
  </integration.config>
  <common>
    <!--[DEBUG < INFO < WARN < ERROR < FATAL]-->
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4Net1211">
        <!--表示log4net的配置直接写在配置文件里-->
        <!--<arg key="configType" value="INLINE"/>-->
        <!--log4net使用独立的配置文件-->
        <arg key="configType" value="FILE-WATCH" />
        <arg key="configFile" value="log4net.config" />
      </factoryAdapter>
    </logging>
  </common>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
