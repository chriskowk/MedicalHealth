<?xml version="1.0" encoding="utf-8" ?>
<quartz xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 				version="1.0"
				overwrite-existing-jobs="true">
  <job>
    <job-detail>
      <name>MyJobE</name>
      <group>MyGroupE</group>
      <job-type>MyJob.TaskJobE, MyJob</job-type>
    </job-detail>
    <trigger>
      <cron>
        <name>cronMyJobE</name>
        <group>cronMyJobE</group>
        <job-name>MyJobE</job-name>
        <job-group>MyJobE</job-group>
        <!--秒 分 小时 月内日期 月 周内日期 年（可选字段）-->
        <!--周一到周五每天的8点到20点，每一分钟触发一次-->
        <!--<cron-expression>0 0/1 8-20 ? * MON-FRI</cron-expression>-->
        <!--每天21点40分触发-->
        <cron-expression>0 50 16 ? * *</cron-expression>
      </cron>
    </trigger>
  </job>
</quartz>
